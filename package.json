{
  "name": "@typescript-exercise/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "setup": "cross-env NODE_ENV=development ts-node scripts/setup-env.ts",
    "setup:docker": "cross-env RUNNING_IN_DOCKER=true node -r ts-node/register scripts/setup-env.ts",
    "postinstall": "npm run setup",
    "prepare": "husky install",
    "serve:frontend": "nx serve frontend",
    "serve:backend": "dotenv -e libs/backend/core/src/lib/config/env/development.env -- nx serve backend",
    "serve:docker": "docker-compose -f ./docker/docker-compose.yml --env-file libs/backend/core/src/lib/config/env/development.env up",
    "prisma:generate": "prisma generate --schema libs/backend/data-access/src/lib/prisma/schema.prisma",
    "prisma:migrate": "dotenv -e libs/backend/core/src/lib/config/env/development.env -- prisma migrate dev --schema libs/backend/data-access/src/lib/prisma/schema.prisma",
    "prisma:deploy": "dotenv -e libs/backend/core/src/lib/config/env/development.env -- prisma migrate deploy --schema libs/backend/data-access/src/lib/prisma/schema.prisma",
    "prisma:studio": "dotenv -e libs/backend/core/src/lib/config/env/development.env -- prisma studio --schema libs/backend/data-access/src/lib/prisma/schema.prisma"
  },
  "private": true,
